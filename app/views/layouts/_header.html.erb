<header>
    <div class="container">
        <% if logged_in? %>
            <%= link_to 'Create Event', new_event_path %>
            <%= link_to 'Log out', request.base_url + '/logout' %>
        <% else %>
            <%= link_to 'Log in', request.base_url + '/login' %>
            <%= link_to 'Register', new_user_path %>
        <% end %>
    </div>
</header>